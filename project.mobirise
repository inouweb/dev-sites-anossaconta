div class=\"menu-content-top\">\n            <div class=\"menu-content-left\">\n                <div class=\"info-widget\">\n                    <span mbr-icon class=\"widget-icon mbr-iconfont mobi-mbri-map-pin mobi-mbri\" style=\"\n                            color: rgb(158, 158, 158);\n                            fill: rgb(158, 158, 158);\n                        \"></span>\n                    <div class=\"widget-content\" mbr-theme-style=\"display-4\">\n                        <p class=\"location-title mbr-fonts-style\" data-app-selector=\".location-title\" mbr-theme-style=\"display-4\">\n                            Endere√ßo da Empresa</p>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"menu-content-right\">\n                <div class=\"info-widget\">\n                    <span mbr-icon class=\"widget-icon mbr-iconfont mobi-mbri-letter mobi-mbri\" style=\"\n                            color: rgb(158, 158, 158);\n                            fill: rgb(158, 158, 158);\n                        \"></span>\n                    <div class=\"widget-content\" mbr-theme-style=\"display-4\">\n                        <p class=\"widget-title mbr-fonts-style\" data-app-selector=\".widget-title, .widget-icon\" mbr-theme-style=\"display-4\">contato@anossaconta.com.br</p>\n                    </div>\n                </div>\n                <div class=\"info-widget\">\n                    <span mbr-icon class=\"widget-icon mbr-iconfont mobi-mbri-phone mobi-mbri\" style=\"\n                            color: rgb(158, 158, 158);\n                            fill: rgb(158, 158, 158);\n                        \"></span>\n                    <div class=\"widget-content\" mbr-theme-style=\"display-4\">\n                        <p class=\"widget-title mbr-fonts-style\" data-app-selector=\".widget-title, .widget-icon\" mbr-theme-style=\"display-4\">(11) 9 5242 - 5897</p>\n                    </div>\n                </div>\n\n         